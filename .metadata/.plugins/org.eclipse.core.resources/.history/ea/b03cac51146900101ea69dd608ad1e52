package helloworld;

class Employee {
    int id;
    String name;
    double salary;

    Employee(int id, String name, double salary) {
        this.id = id;
        this.name = name;
        this.salary = salary;
    }

    public String toString() {
        return id + " - " + name + " - â‚¹" + salary;
    }
}

public class Test1 {
    public static void main(String[] args) {
        Employee[] employees = new Employee[3];

        employees[0] = new Employee(1, "Rahul", 50000);
        employees[1] = new Employee(2, "Hammad", 70000);
        employees[2] = new Employee(3, "Sneha", 40000);

       
        java.util.Arrays.sort(employees, (a, b) -> Double.compare(a.salary, b.salary));

    
        System.out.println("Sorted by Salary:");
        for (Employee e : employees) {
            System.out.println(e);
        }
    }
}

